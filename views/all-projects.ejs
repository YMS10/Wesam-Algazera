<!DOCTYPE html>
<html lang="ar" dir="rtl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>visibility-studies</title>
    <!-- Main Css File -->
    <link rel="stylesheet" href="css/all-projects.css">
    <!-- Normalize File -->
    <link rel="stylesheet" href="css/normalize.css">
    <!-- Global Rules File -->
    <link rel="stylesheet" href="css/reset.css">
    <!-- Header File -->
    <link rel="stylesheet" href="css/header.css">
    <!-- FrameWork File -->
    <link rel="stylesheet" href="css/framework.css">
    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400;500;600;700;900" rel="stylesheet">
    <!-- Font Awesome Library -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <%- include ('partials/header.ejs') %>
    <!-- Start Header Image -->
    <div class="container">
        <img class="header-img" src="images/carousel 02.jpg" alt="">
    </div>
    <!-- End Header Image -->
    <!-- Start All Projects -->
    <div class="all-projects">
        <div class="container">
            <h2 class="h1">دراسات الجدوى</h2>
            <div class="info">
                <% projects.forEach((project) => { %>
                    <div class="project">
                      <div class="image-overlay">
                        <img src="data:image/jpeg;base64,<%= project.image %>" style="width: 373px; height: 248px;" alt="">
                        <div class="text-overlay">
                          <h3 id="project-title"><%= project.title %></h3>
                          <div class="more-info">
                            <a href="/project/<%= project._id %>"> <!-- Add this link -->
                                <h6>Find Out More</h6>
                                <button>+</button>
                            </a>
                          </div>
                        </div>
                      </div>
                    </div>
                  <% }) %>
            </div>
        </div>
    </div>
    <!-- End All Projects -->
    <%- include ('partials/footer.ejs') %>
    <!-- Jquery Cdn -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.0/jquery.min.js" integrity="sha512-3gJwYpMe3QewGELv8k/BX9vcqhryRdzRMxVfq6ngyWXwo03GFEzjsUm8Q7RZcHPHksttq7/GFoxjCVUjkjvPdw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <!-- Main Js File -->
    <!-- <script src="js/header.js"></script> -->
</body>
</html>